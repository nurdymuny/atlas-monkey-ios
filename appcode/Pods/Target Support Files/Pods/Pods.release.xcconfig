GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/AprilSDK" "${PODS_ROOT}/Headers/Public/MBProgressHUD" "${PODS_ROOT}/Headers/Public/Reachability"
LIBRARY_SEARCH_PATHS = $(PODS_ROOT)/AprilSDK "$(PODS_ROOT)/AprilSDK/AprilSDK"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/AprilSDK" -isystem "${PODS_ROOT}/Headers/Public/MBProgressHUD" -isystem "${PODS_ROOT}/Headers/Public/Reachability"
OTHER_LDFLAGS = $(inherited) -ObjC -all_load -l"AprilBeaconSDK" -l"MBProgressHUD" -l"Reachability" -framework "CoreGraphics" -framework "SystemConfiguration"
PODS_ROOT = ${SRCROOT}/Pods